{
	"learning_rate": 0.00005,
	"minibatch_size": 50,
	"batch_size": 64,
	"n_actions": 4,
	"t_max": 1000,
	"train_epochs": 20000,
	"max_buf_size": 40000,
	"buf_size": 2000,
	"env": "gym_snake:snake-v0",
	"env_name": "snake",
	"use_raw_env": false,
	"multistep": 5,
	"update_freq": 4,
	"update_steps": 1,
	"alpha": 0.6,
	"beta": 0.5,
	"delta": 1.0,
	"target_delay": 750,
	"algo": [
		"DDQN", "Dueling"
	],
	"callbacks": [
		{
			"type": "PretrainCallback",
			"kwargs": {
				"episodes": 500, 
				"train_epochs": 6
			}
		},
		{
			"type": "InitBufferCallback",
			"kwargs": {
				"episodes": 500
			}
		},
		{
			"type": "AnnealingSchedulerCallback",
			"kwargs": {
				"target": "epsilon",
				"schedule": [
					{
						"type": "Schedule",
						"kwargs": {
							"length": 1000,
							"start_val": 0.4,
							"end_val": 0.1,
							"fn": "linear"
						}
					},
					{
						"type": "Schedule",
						"kwargs": {
							"length": 9000,
							"start_val": 0.1,
							"end_val": 0.01,
							"fn": "linear"
						}
					}
				]
			}
		}
	]
}
